[${master-name}]
${master-ip}

[${master-name}:vars]
ansible_user=${user-name}
ansible_ssh_private_key_file=/home/ubuntu/.ssh/id_rsa
ansible_ssh_common_args='-o StrictHostKeyChecking=no'

[${slave-name}]
${slave-ip}

[${slave-name}:vars]
ansible_user=${user-name}
ansible_ssh_private_key_file=/home/ubuntu/.ssh/id_rsa
ansible_ssh_common_args='-o StrictHostKeyChecking=no'